{
  "name": "wasm-image-optimization",
  "description": "Optimize images with wasm on edge runtime",
  "version": "0.0.5",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts"
    },
    "./edge": {
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/cjs/index.d.ts"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn tsc -b test && node test",
    "build": "tsc && tsc -p ./tsconfig.esm.json && cpy esm dist",
    "build:wasm": "cpy --cwd=src *.d.ts ../../dist && bash -c make ",
    "lint:fix": "eslint --fix src/ && prettier -w src",
    "clone:webp": "git clone https://github.com/webmproject/libwebp"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webp",
    "encode",
    "wasm",
    "cloudflare",
    "workers",
    "WebAssembly"
  ],
  "repository": "https://github.com/node-libraries/wasm-image-optimization",
  "author": "SoraKumo <info@croud.jp>"
}
